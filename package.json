{
  "name": "available-versions",
  "description": "Returns a promise with new versions higher than given for a npm module",
  "version": "0.0.0-semantic-release",
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "bin": {
    "available": "bin/available.js",
    "versions": "bin/available.js",
    "vers": "bin/available.js",
    "releases": "bin/available.js"
  },
  "bugs": {
    "url": "https://github.com/bahmutov/available-versions/issues"
  },
  "config": {
    "next-update": {
      "skip": [
        "q"
      ]
    },
    "pre-git": {
      "commit-msg": "simple",
      "pre-commit": [
        "npm test"
      ],
      "pre-push": [
        "npm run size"
      ],
      "post-commit": [],
      "post-merge": []
    }
  },
  "contributors": [],
  "dependencies": {
    "check-more-types": "2.3.0",
    "console.table": "0.4.0",
    "lazy-ass": "1.3.0",
    "lodash": "3.10.1",
    "moment": "2.10.6",
    "npm-utils": "1.0.0",
    "q": "1.4.1",
    "request": "2.67.0",
    "semver": "5.1.0",
    "simple-bin-help": "1.5.0"
  },
  "devDependencies": {
    "coveralls": "2.11.4",
    "git-issues": "1.2.0",
    "grunt": "0.4.5",
    "grunt-cli": "0.1.13",
    "grunt-contrib-jshint": "0.11.3",
    "grunt-nice-package": "0.10.2",
    "gt": "0.9.1",
    "jshint-solid": "0.2.2",
    "jshint-stylish": "2.1.0",
    "lcov-filter": "0.1.1",
    "matchdep": "1.0.0",
    "pre-git": "3.0.0",
    "semantic-release": "6.0.3",
    "time-grunt": "1.2.2"
  },
  "engines": {
    "node": ">= 0.8.0"
  },
  "files": [
    "bin",
    "src",
    "!src/test"
  ],
  "homepage": "https://github.com/bahmutov/available-versions",
  "keywords": [
    "available",
    "check",
    "npm",
    "registry",
    "release",
    "releases",
    "version",
    "versions"
  ],
  "license": "MIT",
  "main": "src/available.js",
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/available-versions.git"
  },
  "scripts": {
    "build": "grunt",
    "commit": "git-issues && commit-wizard",
    "coveralls": "node node_modules/lcov-filter/index.js cover/lcov.info test | ./node_modules/coveralls/bin/coveralls.js",
    "example": "node --harmony bin/available.js manpm",
    "example-tags": "node --harmony bin/available.js publish-tag",
    "issues": "git-issues",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "size": "t=\"$(npm pack .)\"; wc -c \"${t}\"; tar tvf \"${t}\"; rm \"${t}\";",
    "test": "grunt && npm run unit",
    "unit": "gt src/test/*.js"
  }
}
